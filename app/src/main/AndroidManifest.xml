<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.victor.v2ex">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.HotActivity" />
        <activity android:name=".activity.NewesetActivity" />
        <activity
            android:name=".activity.ContentActivity"
            android:parentActivityName=".activity.HotActivity" />
        <activity android:name=".activity.NodeActivity" />
        <activity android:name=".activity.TabActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.AllNodeActivity" />
        <activity android:name=".activity.MemberActivity" />
        <activity android:name=".activity.BigPhotoActivity"></activity>
    </application>

</manifest>